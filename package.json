{
  "name": "kings-league",
  "version": "1.0.3",
  "description": "Kings League Infojobs Project Web & API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev:api": "wrangler dev api/index.js",
    "dev": "astro dev",
    "format": "prettier --write src/**/* scraping/**/*",
    "lint:fix": "eslint . --ext .js,.astro,.ts,.tsx --fix",
    "lint": "eslint . --ext .js,.astro,.ts,.tsx",
    "preview": "astro preview",
    "publish:api": "wrangler publish api/index.js",
    "scrape": "node --no-warnings scraping/index.js",
    "start": "astro dev",
    "test:cov": "vitest run --coverage",
    "test": "vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@astrojs/tailwind": "^2.1.3",
    "astro": "1.9.0",
    "cheerio": "1.0.0-rc.12",
    "hono": "2.7.1",
    "picocolors": "^1.0.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "0.26.3",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-astro": "0.21.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "prettier": "2.8.1",
    "standard": "17.0.0",
    "vite": "4.0.4",
    "vitest": "0.26.3",
    "wrangler": "2.6.2"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:astro/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm test"
    }
  }
}
